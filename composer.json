{
    "name": "kunstmaan/tagging-bundle",
    "type": "symfony-bundle",
    "description": "Uses FabienPennequin/DoctrineExtensions-Taggable to add tagging to the Kunstmaan bundles",
    "keywords": [ "kunstmaan", "cms", "tag", "tagging"],
    "homepage": "https://github.com/Kunstmaan/KunstmaanTaggingBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Kunstmaan Team",
            "homepage": "http://www.kunstmaan.be/",
            "email": "support@kunstmaan.be"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "~2.3",
        "fpn/doctrine-extensions-taggable": "dev-master#ee7245fc7c3012ea1b1ed932f62ca258b39d8bea",
        "kunstmaan/admin-bundle": "2.3.*",
        "kunstmaan/adminlist-bundle": "2.3.*",
        "kunstmaan/media-bundle": "2.3.*",
        "kunstmaan/node-bundle": "2.3.*"
    },
    "autoload": {
        "psr-0": { "Kunstmaan\\TaggingBundle": "" }
    },
    "target-dir": "Kunstmaan/TaggingBundle",
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
